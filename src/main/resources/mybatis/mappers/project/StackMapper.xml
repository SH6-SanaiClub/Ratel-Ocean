<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sanaiclub.project.dao.StackMapper">

    <!--  모든 스택 데이터를 가져오기  -->
    <select id="findAll" resultType="com.sanaiclub.project.model.dto.StackDto">
        SELECT stack_id, stack_name, category
        FROM stacks
        ORDER BY stack_id ASC
    </select>

</mapper>